<div class="modal hide" id="modalNewTechnique">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal">x</button>
		<h3>Add New Technique</h3>
	</div>
	<div class="modal-body">
		<p>
			<form class="form-horizontal">
				<fieldset>
					<div id="divTechniqueId" class="control-group">
						<label style="text-align: left" class="control-label">Id</label> 
						<div class="controls">
							<input id="inputTechniqueId" type="text" 
								onchange="$('#divTechniqueId').attr('class', 'control-group'); $('#errorValTechniqueId').html(null)">
							<p class="help-block" id="errorValTechniqueId"></p>
						</div>
					</div>
					<div id="divTechniqueDesc" class="control-group">
						<label style="text-align: left" class="control-label">Description</label> 
						<div class="controls">
							<input id="inputTechniqueDescription" type="text" 
								onchange="$('#divTechniqueDesc').attr('class', 'control-group'); $('#errorValTechniqueDesc').html(null)">
							<p class="help-block" id="errorValTechniqueDesc"></p>
						</div>
					</div>
					<div class="control-group">
	      				<div class="controls">
							<a class="btn btn-primary" id="addTechnique">Add</a>
						</div>
					</div>
				</fieldset>
			</form>
		</p>
		
	</div>
	
	<div class="modal-footer">
		<a href="#" class="btn" data-dismiss="modal">Close</a>
	</div>
	
</div>