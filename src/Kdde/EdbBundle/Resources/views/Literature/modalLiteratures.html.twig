<div class="modal hide" id="viewLiteratureModal">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal">x</button>
		<h3>Literature</h3>
	</div>
	<div class="modal-body">
		<p>
		<table id="tableLiterature" class="table table-condensed table-bordered">
			<thead>
				<tr>
					<th class="span3">Code</th>
					<th class="span7">Description</th>
					<th class="span1">Add</th>
				</tr>
			</thead>
			<tbody>
				{% for lit in literatures %}
					<tr>
						<td> {{ lit.id }}</td>	
						<td> {{ lit.description }}</td>
						<td><button type="button" class="btn" onclick="addToLiterature('{{ lit.id }}')">add</button></td>				
					</tr>
				{% endfor %}
			</tbody>
		</table>
		
	</div>
	<div class="modal-footer">
		<a href="#" class="btn" data-dismiss="modal">Close</a>
	</div>
</div>